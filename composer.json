{
  "name": "doc2test/doc2test",
  "description": "Doc2Test is a documentation testing framework for PHP. It tests code snippets in your docs",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "league/commonmark": "0.16",
    "symfony/console": "^3.3",
    "monolog/monolog": "^1.23",
    "matthiasmullie/path-converter": "^1.1",
    "league/plates": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Doc2Test\\Doc2Test\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Doc2Test\\Doc2Test\\Test\\": "test/unit"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "bin": [
    "doc2test"
  ],
  "authors": [
    {
      "name": "Alexey Karapetov",
      "email": "karapetov@gmail.com"
    }
  ]
}
