{
  "name": "f3ath/doc2test",
  "description": "Doc2Test is a documentation testing framework for PHP. It tests code snippets in your docs",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.0",
    "erusev/parsedown": "1.6.*",
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Doc2Test\\Doc2Test\\": "src"
    }
  },
  "scripts": {
    "test": "rm -rf output && ./doc2test && phpunit -c output/phpunit.xml --coverage-text"
  },
  "authors": [
    {
      "name": "Alexey Karapetov",
      "email": "karapetov@gmail.com"
    }
  ]
}
